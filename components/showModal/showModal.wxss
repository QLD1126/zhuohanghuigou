/* components/showModel/showModel.wxss */
.mask{
position: absolute;
top: 0;
width: 750rpx;
height: 100%;
background: rgba(161, 161, 161, 0.582);
}
.model{
  margin: 198rpx auto;
  width: 563rpx;
  height: 312rpx;
  background: #fff;
  font-size:30rpx ;
  color: #333;
  text-align: center;
  border-radius: 10rpx;
  overflow: hidden;
  z-index: 5;
}
.model view{
  height:222rpx;
  line-height: 222rpx;
}
.model text{
  display: inline-block;
  height: 90rpx;width: 281.5rpx;
  line-height: 90rpx;
  background: rgb(236, 234, 234);
}
.model>text:last-child{
  border-left: 1rpx solid #cac8c8;
}